# problemset2

# Read First Person
print("First person, ")
user_input1 = input("Enter a string // What are you?: ")
user_input11 = input("Enter a string // What is him ")
# Read Second Person
print("Second person")
user_input2 = input("Enter a string // What are you?:")
user_input22 = input("Enter a string  // What is him?:")

def check_inputs(user_input1, user_input11, user_input2, user_input22):
    # Check if the first two inputs are "knight" and "knave"
    # and the second two inputs are both "knight"
    if (user_input1 == "knight" and user_input11 == "knave" and
        user_input2 == "knight" and user_input22 == "knight"):
        return True
    else:
        return False

# Example usage
result = check_inputs(user_input1, user_input11, user_input2, user_input22)
print(result)
